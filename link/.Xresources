! Xft font config
Xft.dpi:96
Xft.antialias:true
Xft.rgba:rgb
Xft.hinting:true
Xft.hintstyle:hintslight
Xft.lcdfilter:lcddefault

! urxvt config
URxvt*loginShell:true
URxvt*inheritPixmap:true
URxvt*tintColor:white
URxvt*transparent:true
URxvt*shading:20
URxvt*scrollBar:true
URxvt*scrollBar_right:true
URxvt*scrollColor:#202020
URxvt*transpscrollbar:true
URxvt*saveLines:65535
URxvt*font: xft:Monaco:style=Regular:size=11:antialias=true:hinting=true
URxvt*letterSpace: -1
! Tango theme
URxvt*background: #000000
URxvt*foreground: #FFFFFF
! black
URxvt*color0  : #2E3436
URxvt*color8  : #555753
! red
URxvt*color1  : #CC0000
URxvt*color9  : #EF2929
! green
URxvt*color2  : #4E9A06
URxvt*color10 : #8AE234
! yellow
URxvt*color3  : #C4A000
URxvt*color11 : #FCE94F
! blue
URxvt*color4  : #3465A4
URxvt*color12 : #729FCF
! magenta
URxvt*color5  : #75507B
URxvt*color13 : #AD7FA8
! cyan
URxvt*color6  : #06989A
URxvt*color14 : #34E2E2
! white
URxvt*color7  : #D3D7CF
URxvt*color15 : #EEEEEC
